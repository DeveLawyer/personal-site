{
  "name": "personal-site",
  "alias": "site.sergiodxa.com",
  "type": "static",
  "public": true,
  "files": [
    "components",
    "data",
    "layouts",
    "lib",
    "pages",
    "plugins",
    "scripts",
    "static",
    "Dockerfile",
    "next.config.js",
    "now.json",
    "package.json",
    "yarn.lock"
  ],
  "static": {
    "headers": [
      {
        "source": "_next/**/*.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, immutable, max-age=604800"
          }
        ]
      },
      {
        "source": "static/**/*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, immutable, max-age=604800"
          }
        ]
      },
      {
        "source": "sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store"
          }
        ]
      },
      {
        "source": "atom",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, stale-while-revalidate=3600"
          }
        ]
      },
      {
        "source": "**/*.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, stale-while-revalidate=3600"
          }
        ]
      }
    ],
    "directoryListing": false,
    "trailingSlash": false
  }
}
